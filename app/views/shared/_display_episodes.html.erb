<div id="episodes-title">
  <h4>Episodes</h4>
  <h5>
    <% if @show.title %> <%= @show.title %> <% else %>Your Feed<% end %>
    </h5>
</div>
<div id="episodes">
  <% @episodes.each do |episode| %>
  <div class="episode">
    <%= link_to episode.title, episode.url %>
    <% if episode.show.show_img %>
      <%= image_tag(episode.show.show_img) %>
    <% end %>
  </div>
  <% end %>
</div>
